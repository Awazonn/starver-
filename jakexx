        ctx.bezierCurveTo(169.5 * scale, 83 * scale, 169.5 * scale, 83 * scale, 153 * scale, 96 * scale);
        ctx.closePath ();
        fill_path (ctx, color[3]); //rgb(106, 217, 232)

        /*ctx.globalAlpha = 0.3;
  ctx.beginPath ();
