            ctx.translate (0, -36 * scale);
            ctx.save ()
            ctx.translate (165.25 * scale, 106.84375 * scale);
            round_rect (ctx, -56.5 * scale, -41.5 * scale, 113 * scale, 83 * scale, 18 * scale);
            ctx.restore ();
