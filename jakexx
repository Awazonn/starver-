    var AUDIO = {

        DIG : {

            sound : null,
            path : ["audio/ambience.mp3"],

            sprite : {

                "d0" : [90086, 300, false],
                "d1" : [90391, 300, false],
                "d2" : [90684, 300, false],
            },

            sample : {

                "d0" : CONST_AUDIO.PLAYERS,
                "d1" : CONST_AUDIO.PLAYERS,
                "d2" : CONST_AUDIO.PLAYERS,
            },
        },

        PUNCH : {

            sound : null,
            path : ["audio/ambience.mp3"],

            sprite : {

                "p0" : [89303, 300, false],
