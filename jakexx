        can.width = 440 * scale;
        can.height = 388 * scale;



        ctx.globalAlpha = (draw_shadow) ? 0.5 : 1;
        ctx.translate (5 * scale, 5 * scale);

        circle (ctx, 220 * scale, 201 * scale, 170.41713528867922 * scale);
