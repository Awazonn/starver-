    SOUND[INV.SWORD_AMETHYST]    = CONST_AUDIO.MISC;
    SOUND[INV.SWORD_REIDITE]     = CONST_AUDIO.MISC;
    SOUND[INV.CRAB_SPEAR]        = CONST_AUDIO.MISC;
    SOUND[INV.WOOD_SPEAR]        = CONST_AUDIO.MISC;
    SOUND[INV.SPEAR]             = CONST_AUDIO.MISC;
