        ctx.bezierCurveTo(116 * scale, 216.75 * scale, 115.5 * scale, 217.375 * scale, 116 * scale, 216.75 * scale);
        ctx.closePath ();
        fill_path (ctx, color[5], color[5], 2 * scale); //#f0d200 #f0d200




