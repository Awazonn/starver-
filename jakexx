        ctx.bezierCurveTo(56 * scale, 82 * scale, 73 * scale, 82 * scale, 93 * scale, 89 * scale);
        fill_path (ctx, undefined, color[0], 40 * scale); //#d32eda #a2241f

        ctx.beginPath ();
