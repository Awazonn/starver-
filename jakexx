        ctx.closePath ();
        fill_path (ctx, color[0], color[1], 2 * scale); //#000000 #000000
        ctx.beginPath ();
        ctx.lineCap = "round";
        ctx.lineJoin = "round";
        ctx.moveTo (42.83332824707031 * scale, 23.078125 * scale);
