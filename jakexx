        var wt     = scale * 200;
        var ht     = scale * 180;
        var r      = scale * 80;
        var wt2    = wt / 2;
        var ht2    = ht / 2;
        var width  = 4;

        can.width  = w;
        can.height = h;

        can.width = 419 * scale;
        can.height = 412 * scale;

        if (i == 1){
