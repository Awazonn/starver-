        ctx.bezierCurveTo(209 * scale, 169.5 * scale, 209 * scale, 169.5 * scale, 194 * scale, 177 * scale);
        ctx.bezierCurveTo(213.5 * scale, 175 * scale, 213.5 * scale, 175 * scale, 233 * scale, 173 * scale);
        ctx.closePath ();
        fill_path (ctx, color[2]); //#9e793d #5f461f

        return can;
    }

