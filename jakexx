        ctx.lineJoin = "round";
        ctx.moveTo (23.833328247070312 * scale, 24.078125 * scale);
        ctx.bezierCurveTo(20.833328247070312 * scale, 34.078125 * scale, 23.833328247070312 * scale, 40.078125 * scale, 27.833328247070312 * scale, 43.078125 * scale);
