        ctx.beginPath ();
        ctx.lineCap = "round";
        ctx.lineJoin = "round";
        ctx.moveTo (61.5 * scale, 78.421875 * scale);
        ctx.bezierCurveTo(57.5 * scale, 73.421875 * scale, 51.5 * scale, 66.421875 * scale, 45.5 * scale, 68.421875 * scale);
