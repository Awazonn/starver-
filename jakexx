        ctx.bezierCurveTo(148 * scale, 83 * scale, 145 * scale, 84 * scale, 143 * scale, 77 * scale);
        ctx.closePath ();
        fill_path (ctx, color[0]); //#9e793d #5f461f
        ctx.beginPath ();
        ctx.lineCap = "round";
        ctx.lineJoin = "round";
        ctx.moveTo (149 * scale, 92 * scale);
