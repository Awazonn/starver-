            ctx.closePath ();
            fill_path (ctx, color[5]); //#9e793d #5f461f
            ctx.translate ( -5 * scale, 55 * scale);

            ctx.beginPath ();
            ctx.lineCap = "round";
            ctx.lineJoin = "round";
