        AMBIENCE : {
            sound : null,
            path : ["audio/ambience.mp3"],
            sprite : {
                cook        : [100,    2800,  true],
                seaday      : [3100,   8800,  true],
                seanight    : [19882,  8301,  true],
                forestday   : [32604,  21890, true],
                forestnight : [54524,  11393, true],
