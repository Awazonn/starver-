                swim        : [28380,  1797,  true],
                sand        : [30562,  655,   true],
                walk        : [31658,  813,   true],
                snow        : [66014,  925,   true],
                hwood       : [91022,  400,   false], // hit wood 0
//			hw1         : [91431,  400,   false], // hit wood 1
                hand        : [91431,  400,   false], // hit hand
                hstone      : [92249,  500,   false], // hit minerals 0
//			hm1         : [92777,  500,   false], // hit minerals 0
            },

