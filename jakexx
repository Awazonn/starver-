    var EXTRACTORS = [

        [ITEMS.EXTRACTOR_MACHINE_STONE, INV.STONE],
        [ITEMS.EXTRACTOR_MACHINE_GOLD, INV.GOLD],
        [ITEMS.EXTRACTOR_MACHINE_DIAMOND, INV.DIAMOND],
