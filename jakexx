        ctx.closePath ();
        fill_path (ctx, color[1], color[2], 8 * scale); //#9e793d #5f461f
        circle (ctx, 77.00003051757812 * scale, 104.6761474609375 * scale, 32.55764119219941 * scale);
        fill_path (ctx, color[1], color[2], 8 * scale); //#9e793d #5f461f
        ctx.beginPath ();
        ctx.lineCap = "round";
        ctx.lineJoin = "round";
